<?xml version="1.0" encoding="utf-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:GeekHubApp"
    x:Class="GeekHubApp.AppShell">

    <FlyoutItem Title="Inicio">
        <ShellContent ContentTemplate="{DataTemplate local:InicioPage}" />
    </FlyoutItem>

    <FlyoutItem Title="Dragon Ball">
        <ShellContent ContentTemplate="{DataTemplate local:DragonBallPage}" />
    </FlyoutItem>

    <FlyoutItem Title="Naruto">
        <ShellContent ContentTemplate="{DataTemplate local:NarutoPage}" />
    </FlyoutItem>

    <FlyoutItem Title="Stranger Things">
        <ShellContent ContentTemplate="{DataTemplate local:StrangerThingsPage}" />
    </FlyoutItem>

    <FlyoutItem Title="Mario Bros">
        <ShellContent ContentTemplate="{DataTemplate local:MarioBrosPage}" />
    </FlyoutItem>

</Shell>
